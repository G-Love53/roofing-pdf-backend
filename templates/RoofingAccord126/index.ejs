<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>ACORD 126 - Business Liability Section</title>
    <style>
        @page {
            size: letter;
            margin: 0.5in;
        }
        * {
            box-sizing: border-box;
            margin: 0;
            padding: 0;
        }
        body {
            font-family: Arial, sans-serif;
            font-size: 9pt;
            line-height: 1.2;
            width: 8.5in;
            margin: 0 auto;
            padding: 0.5in;
        }
        .header {
            border: 1px solid black;
            padding: 5px;
            margin-bottom: 5px;
        }
        .section {
            border: 1px solid black;
            margin-bottom: 5px;
            padding: 5px;
        }
        .row {
            display: flex;
            width: 100%;
        }
        .cell {
            border-right: 1px solid black;
            padding: 2px 4px;
            min-height: 14pt;
        }
        .cell:last-child {
            border-right: none;
        }
        .w5 { width: 5%; }
        .w10 { width: 10%; }
        .w15 { width: 15%; }
        .w20 { width: 20%; }
        .w25 { width: 25%; }
        .w30 { width: 30%; }
        .w33 { width: 33.33%; }
        .w35 { width: 35%; }
        .w40 { width: 40%; }
        .w45 { width: 45%; }
        .w50 { width: 50%; }
        .w60 { width: 60%; }
        .w65 { width: 65%; }
        .w70 { width: 70%; }
        .w75 { width: 75%; }
        .w80 { width: 80%; }
        .w100 { width: 100%; }
        .checkbox {
            width: 12px;
            height: 12px;
            border: 1px solid black;
            display: inline-block;
            vertical-align: middle;
            margin-right: 4px;
        }
        .checkbox.checked::after {
            content: "X";
            display: block;
            text-align: center;
            line-height: 10px;
            font-weight: bold;
        }
        .bold { font-weight: bold; }
        .center { text-align: center; }
        .right { text-align: right; }
        .small { font-size: 8pt; }
        .tiny { font-size: 7pt; }
        .large { font-size: 10pt; }
        .title { font-size: 11pt; font-weight: bold; text-align: center; }
        .underline { text-decoration: underline; }
        .border-top { border-top: 1px solid black; }
        .border-bottom { border-bottom: 1px solid black; }
        .no-border { border: none !important; }
        .label { font-size: 7pt; }
        .value { font-weight: bold; }
        .num { font-family: 'Courier New', monospace; }
    </style>
</head>
<body>
    <%
    // === COMPAT SHIM (safe; no layout changes) ===
    (function () {
        // Dates (support old/new keys)
        const CURR_DATE = data.current_date || data.date || "";
        const EFF_DATE = data.policy_period_from || data.effective_date || data.effectiveDate || "";
        const EXP_DATE = data.to || data.expiration_date || data.expirationDate || "";

        data.current_date = CURR_DATE;
        data.date = CURR_DATE;
        data.effective_date = EFF_DATE;
        data.effectiveDate = EFF_DATE;
        data.expiration_date = EXP_DATE;
        data.expirationDate = EXP_DATE;

        // NEW MAPPINGS - INSIDE THE FUNCTION
        if (data.applicant_address && !data.applicant_street) {
            data.applicant_street = data.applicant_address;
        }
        
        data.full_time_staff = data.full_time_employees || "";
        data.part_time_staff = data.part_time_employees || "";
        data.paid_to_subcontractors = data.subcontractor_cost || "";
        data.ai_name_1 = data.certificate_recipients_name_1 || "";
        data.interest_name = data.certificate_recipients_name_1 || "";

        // Continue with existing code
        const street = data.applicant_street || data.premise_address || data.applicant_mailing_address || "";
        const city = data.applicant_city || data.premise_city || data.applicant_mailing_city || "";
        const state = data.applicant_state || data.premise_state || data.applicant_mailing_state || "";
        const zip = data.applicant_zip || data.premise_zip || data.applicant_mailing_zip || "";
        
        data.applicant_street = street;
        data.applicant_city = city;
        data.applicant_state = state;
        data.applicant_zip = zip;
        data.applicant_address_full = street + (city || state || zip ? ", " : "") + 
                                      city + (city && state ? ", " : "") + 
                                      state + (zip ? " " : "") + zip;

        if (street && !data.premise_address) data.premise_address = street;
        if (city && !data.premise_city) data.premise_city = city;
        if (state && !data.premise_state) data.premise_state = state;
        if (zip && !data.premise_zip) data.premise_zip = zip;

        // Policy number
        data.policy_number = data.policy_number || data.quote_number || "";
    })();

    // UTILITIES
    function formatDate(dateStr) {
        if (!dateStr) return "";
        try {
            let date;
            if (dateStr.includes('/')) {
                const parts = dateStr.split('/');
                if (parts.length === 3) {
                    date = new Date(parts[2], parts[0] - 1, parts[1]);
                }
            } else if (dateStr.includes('-')) {
                date = new Date(dateStr);
            } else {
                return dateStr;
            }
            
            if (date && !isNaN(date)) {
                return (date.getMonth() + 1).toString().padStart(2, '0') + '/' +
                       date.getDate().toString().padStart(2, '0') + '/' +
                       date.getFullYear();
            }
        } catch (e) {
            console.error('Date formatting error:', e);
        }
        return dateStr;
    }

    function isChecked(value) {
        if (value === undefined || value === null || value === '') return false;
        const strVal = String(value).toLowerCase();
        return strVal === 'true' || strVal === 'yes' || strVal === 'x' || strVal === 'checked' || strVal === '1';
    }

    function money(value) {
        if (!value || value === '0') return '';
        
        const cleanValue = String(value).replace(/[$,]/g, '');
        const num = parseFloat(cleanValue);
        
        if (isNaN(num)) return value;
        
        return num.toLocaleString('en-US', {
            minimumFractionDigits: 0,
            maximumFractionDigits: 0
        });
    }

    function safe(value, defaultValue = '') {
        return value || defaultValue;
    }
    %>

    <!-- HEADER -->
    <div class="header">
        <div class="row">
            <div class="cell w70">
                <div class="title">ACORD 126 - BUSINESS LIABILITY SECTION</div>
                <div class="small center">Bars, Taverns, Nightclubs & Restaurants</div>
            </div>
            <div class="cell w30 center">
                <div class="small">DATE</div>
                <div class="value"><%= formatDate(data.current_date) %></div>
            </div>
        </div>
    </div>

    <!-- APPLICANT INFO -->
    <div class="section">
        <div class="row">
            <div class="cell w100 bold">APPLICANT INFORMATION</div>
        </div>
        <div class="row border-top">
            <div class="cell w50">
                <div class="label">NAME (First, Middle, Last)</div>
                <div class="value"><%= data.applicant_name || data.insured_name || '' %></div>
            </div>
            <div class="cell w50">
                <div class="label">MAILING ADDRESS</div>
                <div class="value"><%= data.applicant_street || '' %></div>
            </div>
        </div>
        <div class="row border-top">
            <div class="cell w25">
                <div class="label">CITY</div>
                <div class="value"><%= data.applicant_city || '' %></div>
            </div>
            <div class="cell w25">
                <div class="label">STATE</div>
                <div class="value"><%= data.applicant_state || '' %></div>
            </div>
            <div class="cell w25">
                <div class="label">ZIP CODE</div>
                <div class="value"><%= data.applicant_zip || '' %></div>
            </div>
            <div class="cell w25">
                <div class="label">COUNTY</div>
                <div class="value"><%= data.applicant_county || '' %></div>
            </div>
        </div>
    </div>

    <!-- POLICY INFO -->
    <div class="section">
        <div class="row">
            <div class="cell w33">
                <div class="label">POLICY NUMBER</div>
                <div class="value"><%= data.policy_number || '' %></div>
            </div>
            <div class="cell w33">
                <div class="label">EFFECTIVE DATE</div>
                <div class="value"><%= formatDate(data.effective_date) %></div>
            </div>
            <div class="cell w34">
                <div class="label">EXPIRATION DATE</div>
                <div class="value"><%= formatDate(data.expiration_date) %></div>
            </div>
        </div>
    </div>

    <!-- BUSINESS TYPE -->
    <div class="section">
        <div class="row">
            <div class="cell w100 bold">TYPE OF BUSINESS</div>
        </div>
        <div class="row border-top">
            <div class="cell w25">
                <span class="checkbox <%= isChecked(data.bar) ? 'checked' : '' %>"></span> Bar
            </div>
            <div class="cell w25">
                <span class="checkbox <%= isChecked(data.tavern) ? 'checked' : '' %>"></span> Tavern
            </div>
            <div class="cell w25">
                <span class="checkbox <%= isChecked(data.nightclub) ? 'checked' : '' %>"></span> Nightclub
            </div>
            <div class="cell w25">
                <span class="checkbox <%= isChecked(data.restaurant) ? 'checked' : '' %>"></span> Restaurant
            </div>
        </div>
        <div class="row border-top">
            <div class="cell w100">
                <div class="label">DESCRIBE OPERATIONS</div>
                <div class="value"><%= data.operations_description || '' %></div>
            </div>
        </div>
    </div>

    <!-- PREMISES LOCATIONS -->
    <div class="section">
        <div class="row">
            <div class="cell w100 bold">PREMISES / LOCATIONS</div>
        </div>
        <div class="row border-top">
            <div class="cell w5 center bold small">#</div>
            <div class="cell w35 center bold small">ADDRESS</div>
            <div class="cell w20 center bold small">CITY</div>
            <div class="cell w10 center bold small">STATE</div>
            <div class="cell w15 center bold small">ZIP</div>
            <div class="cell w15 center bold small">COUNTY</div>
        </div>
        
        <% for(let i = 1; i <= 3; i++) { %>
        <div class="row border-top">
            <div class="cell w5 center"><%= i %></div>
            <div class="cell w35"><%= data['premise_address_' + i] || (i === 1 ? data.premise_address : '') || '' %></div>
            <div class="cell w20"><%= data['premise_city_' + i] || (i === 1 ? data.premise_city : '') || '' %></div>
            <div class="cell w10 center"><%= data['premise_state_' + i] || (i === 1 ? data.premise_state : '') || '' %></div>
            <div class="cell w15"><%= data['premise_zip_' + i] || (i === 1 ? data.premise_zip : '') || '' %></div>
            <div class="cell w15"><%= data['premise_county_' + i] || (i === 1 ? data.premise_county : '') || '' %></div>
        </div>
        <% } %>
    </div>

    <!-- ENTERTAINMENT -->
    <div class="section">
        <div class="row">
            <div class="cell w100 bold">ENTERTAINMENT</div>
        </div>
        <div class="row border-top">
            <div class="cell w25">
                <span class="checkbox <%= isChecked(data.live_music) ? 'checked' : '' %>"></span> Live Music
            </div>
            <div class="cell w25">
                <span class="checkbox <%= isChecked(data.dj) ? 'checked' : '' %>"></span> DJ
            </div>
            <div class="cell w25">
                <span class="checkbox <%= isChecked(data.dancing) ? 'checked' : '' %>"></span> Dancing
            </div>
            <div class="cell w25">
                <span class="checkbox <%= isChecked(data.karaoke) ? 'checked' : '' %>"></span> Karaoke
            </div>
        </div>
        <div class="row border-top">
            <div class="cell w25">
                <span class="checkbox <%= isChecked(data.comedy) ? 'checked' : '' %>"></span> Comedy
            </div>
            <div class="cell w25">
                <span class="checkbox <%= isChecked(data.adult_entertainment) ? 'checked' : '' %>"></span> Adult Entertainment
            </div>
            <div class="cell w25">
                <span class="checkbox <%= isChecked(data.other_entertainment) ? 'checked' : '' %>"></span> Other
            </div>
            <div class="cell w25">
                <% if(data.other_entertainment_description) { %>
                    <div class="small"><%= data.other_entertainment_description %></div>
                <% } %>
            </div>
        </div>
    </div>

    <!-- SECURITY -->
    <div class="section">
        <div class="row">
            <div class="cell w100 bold">SECURITY</div>
        </div>
        <div class="row border-top">
            <div class="cell w50">
                <div class="label">SECURITY PERSONNEL ON PREMISES?</div>
                <span class="checkbox <%= isChecked(data.security_yes) ? 'checked' : '' %>"></span> Yes
                <span class="checkbox <%= isChecked(data.security_no) ? 'checked' : '' %>"></span> No
            </div>
            <div class="cell w50">
                <div class="label">IF YES, NUMBER OF GUARDS</div>
                <div class="value"><%= data.number_of_guards || '' %></div>
            </div>
        </div>
        <div class="row border-top">
            <div class="cell w50">
                <div class="label">HOURS OF SECURITY COVERAGE</div>
                <div class="value"><%= data.security_hours || '' %></div>
            </div>
            <div class="cell w50">
                <div class="label">TYPE OF SECURITY</div>
                <span class="checkbox <%= isChecked(data.security_armed) ? 'checked' : '' %>"></span> Armed
                <span class="checkbox <%= isChecked(data.security_unarmed) ? 'checked' : '' %>"></span> Unarmed
            </div>
        </div>
    </div>

    <!-- OPERATIONS -->
    <div class="section">
        <div class="row">
            <div class="cell w100 bold">OPERATIONS</div>
        </div>
        <div class="row border-top">
            <div class="cell w33">
                <div class="label">HOURS OF OPERATION</div>
                <div class="value"><%= data.hours_of_operation || '' %></div>
            </div>
            <div class="cell w33">
                <div class="label">DAYS OPEN PER WEEK</div>
                <div class="value"><%= data.days_open || '' %></div>
            </div>
            <div class="cell w34">
                <div class="label">SEASONAL?</div>
                <span class="checkbox <%= isChecked(data.seasonal_yes) ? 'checked' : '' %>"></span> Yes
                <span class="checkbox <%= isChecked(data.seasonal_no) ? 'checked' : '' %>"></span> No
            </div>
        </div>
        <div class="row border-top">
            <div class="cell w50">
                <div class="label">MAXIMUM OCCUPANCY</div>
                <div class="value"><%= data.maximum_occupancy || '' %></div>
            </div>
            <div class="cell w50">
                <div class="label">AVERAGE DAILY CUSTOMERS</div>
                <div class="value"><%= data.average_customers || '' %></div>
            </div>
        </div>
    </div>

    <!-- ALCOHOL SERVICE -->
    <div class="section">
        <div class="row">
            <div class="cell w100 bold">ALCOHOL SERVICE</div>
        </div>
        <div class="row border-top">
            <div class="cell w50">
                <div class="label">LIQUOR LICENSE NUMBER</div>
                <div class="value"><%= data.liquor_license_number || '' %></div>
            </div>
            <div class="cell w50">
                <div class="label">LICENSE TYPE</div>
                <span class="checkbox <%= isChecked(data.beer_wine) ? 'checked' : '' %>"></span> Beer/Wine
                <span class="checkbox <%= isChecked(data.full_bar) ? 'checked' : '' %>"></span> Full Bar
            </div>
        </div>
        <div class="row border-top">
            <div class="cell w100">
                <div class="label">SERVER TRAINING PROGRAM?</div>
                <span class="checkbox <%= isChecked(data.server_training_yes) ? 'checked' : '' %>"></span> Yes
                <span class="checkbox <%= isChecked(data.server_training_no) ? 'checked' : '' %>"></span> No
                <% if(data.server_training_description) { %>
                    <span class="small"> - <%= data.server_training_description %></span>
                <% } %>
            </div>
        </div>
    </div>

    <!-- EMPLOYEES -->
    <div class="section">
        <div class="row">
            <div class="cell w100 bold">EMPLOYEES</div>
        </div>
        <div class="row border-top">
            <div class="cell w25">
                <div class="label">FULL TIME</div>
                <div class="value"><%= data.full_time_employees || '' %></div>
            </div>
            <div class="cell w25">
                <div class="label">PART TIME</div>
                <div class="value"><%= data.part_time_employees || '' %></div>
            </div>
            <div class="cell w25">
                <div class="label">SEASONAL</div>
                <div class="value"><%= data.seasonal_employees || '' %></div>
            </div>
            <div class="cell w25">
                <div class="label">TOTAL</div>
                <div class="value"><%= data.total_employees || '' %></div>
            </div>
        </div>
        <div class="row border-top">
            <div class="cell w50">
                <div class="label">ANNUAL PAYROLL</div>
                <div class="value">$<%= money(data.annual_payroll) %></div>
            </div>
            <div class="cell w50">
                <div class="label">SUBCONTRACTOR COSTS</div>
                <div class="value">$<%= money(data.subcontractor_cost) %></div>
            </div>
        </div>
    </div>

    <!-- SCHEDULE OF HAZARDS -->
    <div class="section">
        <div class="row">
            <div class="cell w100 bold">SCHEDULE OF HAZARDS</div>
        </div>
        <div class="row border-top">
            <div class="cell w40 center bold small">HAZARD</div>
            <div class="cell w20 center bold small">CLASSIFICATION</div>
            <div class="cell w10 center bold small">CODE</div>
            <div class="cell w10 center bold small">PREMIUM BASIS</div>
            <div class="cell w10 center bold small">RATE</div>
            <div class="cell w10 center bold small">EXPOSURE</div>
        </div>
        
        <!-- Row 1 - Total Sales -->
        <div class="row border-top">
            <div class="cell w40">Premises Operations - Total Sales</div>
            <div class="cell w20 center">58161</div>
            <div class="cell w10 center">001</div>
            <div class="cell w10 center">Sales</div>
            <div class="cell w10 right"></div>
            <div class="cell w10 right num">$<%= money(data.total_sales || '') %></div>
        </div>
        
        <!-- Row 2 - Alcohol Sales -->
        <div class="row border-top">
            <div class="cell w40">Liquor Liability - Alcohol Sales</div>
            <div class="cell w20 center">58160</div>
            <div class="cell w10 center">002</div>
            <div class="cell w10 center">Sales</div>
            <div class="cell w10 right"></div>
            <div class="cell w10 right num">$<%= money(data.alcohol_sales || '') %></div>
        </div>
        
        <!-- Row 3 - Square Footage -->
        <div class="row border-top">
            <div class="cell w40">Property - Square Footage</div>
            <div class="cell w20 center">61217</div>
            <div class="cell w10 center">003</div>
            <div class="cell w10 center">Area</div>
            <div class="cell w10 right"></div>
            <div class="cell w10 right num"><%= data.square_footage || '' %></div>
        </div>
        
        <!-- Additional Rows -->
        <% for(let i = 4; i <= 6; i++) { %>
        <div class="row border-top">
            <div class="cell w40"><%= data['hazard_' + i] || '' %></div>
            <div class="cell w20 center"><%= data['class_' + i] || '' %></div>
            <div class="cell w10 center"><%= data['code_' + i] || '' %></div>
            <div class="cell w10 center"><%= data['basis_' + i] || '' %></div>
            <div class="cell w10 right"><%= data['rate_' + i] || '' %></div>
            <div class="cell w10 right num"><%= data['exposure_' + i] || '' %></div>
        </div>
        <% } %>
    </div>

    <!-- LIABILITY LIMITS -->
    <div class="section">
        <div class="row">
            <div class="cell w100 bold">LIABILITY LIMITS</div>
        </div>
        <div class="row border-top">
            <div class="cell w50">
                <div class="label">EACH OCCURRENCE</div>
                <div class="value">$<%= money(data.each_occurrence_limit || '1000000') %></div>
            </div>
            <div class="cell w50">
                <div class="label">GENERAL AGGREGATE</div>
                <div class="value">$<%= money(data.general_aggregate_limit || '2000000') %></div>
            </div>
        </div>
        <div class="row border-top">
            <div class="cell w50">
                <div class="label">PRODUCTS/COMPLETED OPS AGGREGATE</div>
                <div class="value">$<%= money(data.products_ops_limit || '2000000') %></div>
            </div>
            <div class="cell w50">
                <div class="label">PERSONAL & ADVERTISING INJURY</div>
                <div class="value">$<%= money(data.personal_adv_limit || '1000000') %></div>
            </div>
        </div>
        <div class="row border-top">
            <div class="cell w50">
                <div class="label">DAMAGE TO RENTED PREMISES</div>
                <div class="value">$<%= money(data.rented_premises_limit || '100000') %></div>
            </div>
            <div class="cell w50">
                <div class="label">MEDICAL EXPENSE (ANY ONE PERSON)</div>
                <div class="value">$<%= money(data.medical_expense_limit || '5000') %></div>
            </div>
        </div>
    </div>

    <!-- LIQUOR LIABILITY LIMITS -->
    <div class="section">
        <div class="row">
            <div class="cell w100 bold">LIQUOR LIABILITY LIMITS</div>
        </div>
        <div class="row border-top">
            <div class="cell w50">
                <div class="label">EACH COMMON CAUSE</div>
                <div class="value">$<%= money(data.liquor_each_cause_limit || '1000000') %></div>
            </div>
            <div class="cell w50">
                <div class="label">AGGREGATE</div>
                <div class="value">$<%= money(data.liquor_aggregate_limit || '1000000') %></div>
            </div>
        </div>
    </div>

    <!-- LOSS HISTORY -->
    <div class="section">
        <div class="row">
            <div class="cell w100 bold">LOSS HISTORY (Past 5 Years)</div>
        </div>
        <div class="row border-top">
            <div class="cell w20 center bold small">DATE</div>
            <div class="cell w40 center bold small">DESCRIPTION</div>
            <div class="cell w20 center bold small">AMOUNT PAID</div>
            <div class="cell w20 center bold small">AMOUNT RESERVED</div>
        </div>
        
        <% for(let i = 1; i <= 3; i++) { %>
        <div class="row border-top">
            <div class="cell w20 center"><%= formatDate(data['loss_date_' + i]) %></div>
            <div class="cell w40"><%= data['loss_description_' + i] || '' %></div>
            <div class="cell w20 right">$<%= money(data['loss_paid_' + i]) %></div>
            <div class="cell w20 right">$<%= money(data['loss_reserved_' + i]) %></div>
        </div>
        <% } %>
    </div>

    <!-- ADDITIONAL INTERESTS -->
    <div class="section">
        <div class="row">
            <div class="cell w100 bold">ADDITIONAL INTERESTS</div>
        </div>
        <div class="row border-top">
            <div class="cell w5 center bold small">#</div>
            <div class="cell w35 center bold small">NAME</div>
            <div class="cell w35 center bold small">ADDRESS</div>
            <div class="cell w25 center bold small">INTEREST TYPE</div>
        </div>
        
        <% for(let i = 1; i <= 2; i++) { %>
        <div class="row border-top">
            <div class="cell w5 center"><%= i %></div>
            <div class="cell w35"><%= data['ai_name_' + i] || '' %></div>
            <div class="cell w35"><%= data['ai_address_' + i] || '' %></div>
            <div class="cell w25">
                <span class="checkbox <%= isChecked(data['ai_mortgagee_' + i]) ? 'checked' : '' %>"></span> Mortgagee
                <span class="checkbox <%= isChecked(data['ai_addl_insured_' + i]) ? 'checked' : '' %>"></span> Add'l Insured
                <span class="checkbox <%= isChecked(data['ai_loss_payee_' + i]) ? 'checked' : '' %>"></span> Loss Payee
            </div>
        </div>
        <% } %>
    </div>

    <!-- REMARKS -->
    <div class="section">
        <div class="row">
            <div class="cell w100 bold">REMARKS</div>
        </div>
        <div class="row border-top">
            <div class="cell w100" style="min-height: 60px;">
                <%= data.remarks || '' %>
            </div>
        </div>
    </div>

    <!-- SIGNATURE -->
    <div class="section">
        <div class="row">
            <div class="cell w50">
                <div class="label">PRODUCER NAME</div>
                <div class="value"><%= data.producer_name || data.agent_name || '' %></div>
            </div>
            <div class="cell w50">
                <div class="label">PRODUCER SIGNATURE</div>
                <div class="value" style="min-height: 30px; border-bottom: 1px solid black; margin-top: 10px;"></div>
            </div>
        </div>
        <div class="row border-top">
            <div class="cell w50">
                <div class="label">APPLICANT NAME</div>
                <div class="value"><%= data.applicant_name || data.insured_name || '' %></div>
            </div>
            <div class="cell w50">
                <div class="label">APPLICANT SIGNATURE</div>
                <div class="value" style="min-height: 30px; border-bottom: 1px solid black; margin-top: 10px;"></div>
            </div>
        </div>
        <div class="row border-top">
            <div class="cell w50">
                <div class="label">DATE</div>
                <div class="value"><%= formatDate(data.signature_date || data.current_date) %></div>
            </div>
            <div class="cell w50">
                <div class="label">AGENCY</div>
                <div class="value"><%= data.agency_name || '' %></div>
            </div>
        </div>
    </div>

    <!-- FOOTER -->
    <div class="footer" style="margin-top: 20px; text-align: center; font-size: 7pt;">
        <div>ACORD 126 (2016/03)</div>
        <div>© 1988-2016 ACORD CORPORATION. All rights reserved.</div>
        <div>The ACORD name and logo are registered marks of ACORD</div>
    </div>
</body>
</html>
