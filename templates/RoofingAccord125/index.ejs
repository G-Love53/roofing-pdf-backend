      margin-top: 8pt;
    }

    /* ===== Boxed blocks (table/grid) ===== */
    .block {
      border: 1pt solid #000;
      border-top: none;
    }

    .block .row {
      display: grid;
      grid-template-columns: repeat(20, 1fr);
      border-top: 1pt solid #000;
    }

    .block .row:first-child {
      border-top: none;
    }

    .cell {
      padding: 3pt 4pt;
      border-left: 1pt solid #000;
