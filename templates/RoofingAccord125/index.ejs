<!-- ====================== PAGE 1 ====================== -->
<div class="page">
  <!-- ACORD Header -->
  <div class="headerbar">
    <div class="acord">ACORD</div>
    <div class="title">COMMERCIAL INSURANCE APPLICATION</div>
    <div class="formno">125</div>
  </div>

  <!-- Producer Section -->
  <div class="subbar">PRODUCER</div>
  <div class="block">
    <div class="row">
      <div class="cell label w15">PRODUCER</div>
      <div class="cell w50"><%= data.producerName %></div>
      <div class="cell label w15">DATE (MM/DD/YYYY)</div>
      <div class="cell w20"><%= formatDate(data.date) %></div>
    </div>
    <div class="row">
      <div class="cell w65" style="grid-row: span 2;">
        <%= data.producerAddress1 %><br>
        <%= data.producerAddress2 %>
      </div>
      <div class="cell label w15">CARRIER</div>
      <div class="cell w20"><%= data.carrier %></div>
    </div>
    <div class="row" style="margin-top: -1px;">
      <div class="cell label w15" style="grid-column: 14 / span 3;">NAIC CODE</div>
      <div class="cell w20" style="grid-column: 17 / span 4;"><%= data.naicCode %></div>
    </div>
    <div class="row">
      <div class="cell label w15">CONTACT NAME</div>
      <div class="cell w50"><%= data.producerContact %></div>
      <div class="cell label w20">COMPANY POLICY OR PROGRAM NAME</div>
      <div class="cell w15"><%= data.companyPolicyName %></div>
    </div>
    <div class="row">
      <div class="cell label w10">PHONE (A/C, No, Ext)</div>
      <div class="cell w25"><%= data.producerPhone %></div>
      <div class="cell label w10">FAX (A/C, No)</div>
      <div class="cell w20"><%= data.producerFax %></div>
      <div class="cell label w15">POLICY NUMBER</div>
      <div class="cell w20"><%= data.policyNumber %></div>
    </div>
    <div class="row">
      <div class="cell label w10">E-MAIL</div>
      <div class="cell w55"><%= data.producerEmail %></div>
      <div class="cell label w15">UNDERWRITER</div>
      <div class="cell w20"><%= data.underwriter %></div>
    </div>
    <div class="row">
      <div class="cell label w20">UNDERWRITER OFFICE</div>
      <div class="cell w20"><%= data.underwriterOffice %></div>
      <div class="cell label w10">CODE</div>
      <div class="cell w10"><%= data.code %></div>
      <div class="cell label w10">SUBCODE</div>
      <div class="cell w10"><%= data.subcode %></div>
      <div class="cell label w15">AGENCY CUSTOMER ID</div>
      <div class="cell w5"><%= data.agencyCustomerId %></div>
    </div>
  </div>

  <!-- Status of Transaction -->
  <div class="block">
    <div class="row">
      <div class="cell label w20">STATUS OF TRANSACTION</div>
      <div class="cell w10 center">
        <span class="check"><%= data.transactionType === "QUOTE" ? "X" : "" %></span> QUOTE
      </div>
      <div class="cell w10 center">
        <span class="check"><%= data.transactionType === "ISSUE" ? "X" : "" %></span> ISSUE POLICY
      </div>
      <div class="cell w10 center">
        <span class="check"><%= data.transactionType === "RENEW" ? "X" : "" %></span> RENEW
      </div>
      <div class="cell w20 center">
        <span class="check"><%= data.transactionType === "BOUND" ? "X" : "" %></span> BOUND (Give Date and/or Attach Copy)
      </div>
      <div class="cell w10"><%= data.boundDate %></div>
      <div class="cell w10 center">
        <span class="check"><%= data.transactionType === "CHANGE" ? "X" : "" %></span> CHANGE
      </div>
      <div class="cell w10 center">
        <span class="check"><%= data.transactionType === "CANCEL" ? "X" : "" %></span> CANCEL
      </div>
    </div>
    <div class="row">
      <div class="cell label w10">TIME:</div>
